#!/bin/bash

echo "Enter the student's score:"
read score

if [ "$score" -ge 80 ]; then
  echo "Grade: A"
elif [ "$score" -ge 70 ] && [ "$score" -lt 80 ]; then
  echo "Grade: B"
elif [ "$score" -ge 60 ] && [ "$score" -lt 70 ]; then
  echo "Grade: C"
elif [ "$score" -ge 50 ] && [ "$score" -lt 60 ]; then
  echo "Grade: D"
else
  echo "Grade: F (Below 50)"
fi
